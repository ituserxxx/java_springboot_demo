<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.xcx.mapper.SwitchesMapper">
    <select id="CountAll" resultType="int">
        select count(1)
        from switches;
    </select>
    <select id="FindAllSwitches" resultType="org.example.xcx.model.Switches">
        select *
        from switches;
    </select>
</mapper>